{
  "name": "do-functions-cli",
  "version": "1.4.0",
  "description": "CLI tool for scaffolding DigitalOcean Serverless Functions with JavaScript and TypeScript templates, automatic dependency installation, and project.yml configuration.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grantchatterton/do-functions-cli.git"
  },
  "homepage": "https://github.com/grantchatterton/do-functions-cli#readme",
  "bugs": {
    "url": "https://github.com/grantchatterton/do-functions-cli/issues"
  },
  "author": "Grant Chatterton",
  "license": "MIT",
  "engines": {
    "node": ">=20.12.0"
  },
  "bin": {
    "do-functions-cli": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.0.1",
    "commander": "^14.0.2",
    "fs-extra": "^11.3.2",
    "ora": "^9.0.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@semantic-release/git": "^10.0.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "digitalocean",
    "serverless",
    "functions",
    "faas",
    "cli",
    "scaffold",
    "generator",
    "typescript",
    "javascript",
    "nodejs",
    "cloud-functions",
    "serverless-functions",
    "do-functions",
    "function-template",
    "project-generator"
  ]
}
